{
  "name": "oursite-server",
  "version": "0.0.1",
  "description": "oursite server side base express",
  "main": "app.js",
  "scripts": {
    "start": "babel src/app.js",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node src/app.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 4000
      }
    },
    "test": {
      "command": "mocha --recursive --compilers js:babel-register",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "PORT": 4001
      }
    },
    "test:watch": {
      "command": "nodemon --exec 'mocha --recursive --compilers js:babel-register'",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "PORT": 4001
      }
    }
  },
  "author": "Kai",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "mocha": "^3.0.0",
    "mongoose": "^4.5.7",
    "nodemon": "^1.10.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "better-npm-run": "0.0.10",
    "debug": "^2.2.0",
    "jsonwebtoken": "^7.1.7",
    "lodash": "^4.14.1",
    "moment": "^2.14.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0"
  }
}
